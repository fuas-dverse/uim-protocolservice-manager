        {
          "intent_uid": "openweathermap.org:getWeatherAlerts:v1",
          "intent_name": "get_weather_alerts",
          "description": "Get severe weather alerts for a region",
          "http_method": "GET",
          "endpoint_path": "/weather",
          "input_parameters": [
            {
              "name": "q",
              "type": "string",
              "description": "City name or region",
              "required": true,
              "location": "query"
            }
          ],
          "tags": ["weather", "alerts", "warnings"],
          "rateLimit": 60,
          "price": 0.005
        }
      ]
