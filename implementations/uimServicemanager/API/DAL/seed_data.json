{
  "services": [
    {
      "name": "arXiv API",
      "description": "Access to scientific research papers, academic papers, preprints, and scholarly articles from arXiv.org. Search papers on computer science, physics, mathematics, AI, machine learning, and more research topics.",
      "service_url": "https://export.arxiv.org/api",
      "service_logo_url": "https://arxiv.org/static/browse/0.3.4/images/arxiv-logo-one-color-white.svg",
      "service_terms_url": "https://info.arxiv.org/help/api/tou.html",
      "auth_type": "none",
      "intents": [
        {
          "intent_uid": "arxiv.org:searchPapers:v1",
          "intent_name": "search_papers",
          "description": "Search for research papers by query, author, or category",
          "http_method": "GET",
          "endpoint_path": "/query",
          "input_parameters": [
            {
              "name": "search_query",
              "type": "string",
              "description": "Search query (use 'all:keyword' for general search)",
              "required": true,
              "location": "query"
            },
            {
              "name": "start",
              "type": "integer",
              "description": "Starting index for results",
              "required": false,
              "default": 0,
              "location": "query"
            },
            {
              "name": "max_results",
              "type": "integer",
              "description": "Maximum number of results to return",
              "required": false,
              "default": 10,
              "location": "query"
            },
            {
              "name": "sortBy",
              "type": "string",
              "description": "Sort by: relevance, lastUpdatedDate, submittedDate",
              "required": false,
              "default": "relevance",
              "location": "query"
            },
            {
              "name": "sortOrder",
              "type": "string",
              "description": "Sort order: ascending or descending",
              "required": false,
              "default": "descending",
              "location": "query"
            }
          ],
          "output_schema": {
            "type": "object",
            "properties": {
              "papers": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "id": {"type": "string"},
                    "title": {"type": "string"},
                    "authors": {"type": "array"},
                    "summary": {"type": "string"},
                    "published": {"type": "string"},
                    "pdf_url": {"type": "string"}
                  }
                }
              }
            }
          },
          "tags": ["research", "papers", "academic", "search"],
          "rateLimit": 30,
          "price": 0.0
        },
        {
          "intent_uid": "arxiv.org:getPaperDetails:v1",
          "intent_name": "get_paper_details",
          "description": "Get detailed information about a specific paper by arXiv ID",
          "http_method": "GET",
          "endpoint_path": "/query",
          "input_parameters": [
            {
              "name": "id_list",
              "type": "string",
              "description": "arXiv ID (e.g., '2301.12345')",
              "required": true,
              "location": "query"
            }
          ],
          "tags": ["research", "papers", "academic", "details"],
          "rateLimit": 30,
          "price": 0.0
        },
        {
          "intent_uid": "arxiv.org:getRecentPapers:v1",
          "intent_name": "get_recent_papers",
          "description": "Get the most recent papers in a specific category",
          "http_method": "GET",
          "endpoint_path": "/query",
          "input_parameters": [
            {
              "name": "search_query",
              "type": "string",
              "description": "Category query (e.g., 'cat:cs.AI' for AI papers)",
              "required": true,
              "location": "query"
            },
            {
              "name": "max_results",
              "type": "integer",
              "description": "Maximum number of results",
              "required": false,
              "default": 10,
              "location": "query"
            },
            {
              "name": "sortBy",
              "type": "string",
              "description": "Sort by date",
              "required": false,
              "default": "submittedDate",
              "location": "query"
            },
            {
              "name": "sortOrder",
              "type": "string",
              "description": "Descending for most recent",
              "required": false,
              "default": "descending",
              "location": "query"
            }
          ],
          "tags": ["research", "papers", "academic", "recent"],
          "rateLimit": 30,
          "price": 0.0
        }
      ]
    },
